// </CLASS SPECIFIC DROPS>
actor ClassDrop
{
  +NOBLOCKMAP
  +NOSECTOR
  +NOGRAVITY
  +THRUACTORS
   Game Doom
  radius 0
  height 0


}

actor EasyMonsterDropOrClassDrop : ClassDrop
{
 /*States
  {
  Spawn:
	TNT1 A 0 NoDelay A_PrintBold("EasyMonsterDropOrClassDrop")
    TNT1 A 0 NoDelay ACS_NamedExecuteWithResult("ClassDrop")
	stop
  }*/
	States
	{
		Spawn:
			TNT1 A 0
			TNT1 A 0 NoDelay A_Jump(256, "EasyMonsterDrop", "ClassDrop")
		Stop
		EasyMonsterDrop:
			TNT1 A 0
			//TNT1 A 0 NoDelay A_PrintBold("EasyMonsterDrop")
			TNT1 A 0 NoDelay A_SpawnItemEx("EasyMonsterDrop", 0,0,0,0,0,0,0,SXF_ABSOLUTEPOSITION|SXF_NOCHECKPOSITION|SXF_TRANSFERAMBUSHFLAG)
			stop

		ClassDrop:
			TNT1 A 0
			//TNT1 A 0 NoDelay A_PrintBold("ClassDrop")
			TNT1 A 0 NoDelay ACS_NamedExecuteWithResult("ClassDrop")
		stop
	}
}
actor MediumMonsterDropOrClassDrop : ClassDrop
{
 /*States
  {
  Spawn:
	TNT1 A 0 NoDelay A_PrintBold("MediumMonsterDropOrClassDrop")
    TNT1 A 0 NoDelay ACS_NamedExecuteWithResult("ClassDrop")
	stop
  }*/
	States
	{
		Spawn:
			TNT1 A 0
			TNT1 A 0 NoDelay A_Jump(256, "MediumMonsterDrop", "ClassDrop")
		Stop
		MediumMonsterDrop:
			TNT1 A 0
			//TNT1 A 0 NoDelay A_PrintBold("MediumMonsterDrop")
			TNT1 A 0 NoDelay A_SpawnItemEx("MediumMonsterDrop", 0,0,0,0,0,0,0,SXF_ABSOLUTEPOSITION|SXF_NOCHECKPOSITION|SXF_TRANSFERAMBUSHFLAG)
			stop
		ClassDrop:
			TNT1 A 0
			//TNT1 A 0 NoDelay A_PrintBold("ClassDrop")
			TNT1 A 0 NoDelay ACS_NamedExecuteWithResult("ClassDrop")
		stop
  }
}
